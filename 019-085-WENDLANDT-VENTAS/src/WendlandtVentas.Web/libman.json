{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validate",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_es.min.js"
      ]
    },
    {
      "library": "js-cookie@latest",
      "destination": "wwwroot/lib/jscookie",
      "files": [
        "js.cookie.min.js"
      ]
    },
    {
      "library": "jquery.form@4.2.2",
      "destination": "wwwroot/lib/jquery-form",
      "files": [
        "jquery.form.min.js"
      ]
    },
    {
      "library": "select2@4.0.12",
      "destination": "wwwroot/lib/select2",
      "files": [
        "css/select2.min.css",
        "js/select2.min.js"
      ]
    },
    {
      "library": "pace@1.0.2",
      "destination": "wwwroot/lib/pace",
      "files": [
        "pace.min.js"
      ]
    }
  ]
}