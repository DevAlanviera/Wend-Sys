<style type="text/css" class="cssStyles">
    .detailtable td {
        font-size: 13px;
        padding: 4px;
        max-width: 0;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }

    .photo {
        width: 100px;
        height: 100px;
        border-radius: 50px;
        box-shadow: inset 0 0 1px #e0e0e0, inset 0 0 14px rgba(0, 0, 0, 0.2);
    }
</style>
@section Modals {
    <div class="modal modal-focus-first" id="addModal" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("Add")"></div>
    <div class="modal" id="editModal" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("Edit")"></div>
    <div class="modal" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("Delete")"></div>
    <div class="modal" id="importModal" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("UploadXlsxView")"></div>

    <div class="modal" id="addContact" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("AddContactView")"></div>
    <div class="modal" id="viewContacts" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("ContactsView")"></div>
    <div class="modal" id="editContact" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("EditContactView")"></div>
    <div class="modal" id="deleteContact" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("DeleteContactView")"></div>

    <div class="modal" id="addAddress" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("AddAddressView")"></div>
    <div class="modal" id="viewAddresses" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("AddressesView")"></div>
    <div class="modal" id="editAddress" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("EditAddressView")"></div>
    <div class="modal" id="deleteAddress" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("DeleteAddressView")"></div>

    <div class="modal" id="addComments" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("AddCommentsView")"></div>
    <div class="modal" id="viewComments" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("CommentsView")"></div>
    <div class="modal" id="editComments" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("EditCommentsView")"></div>
    <div class="modal" id="deleteComments" tabindex="-1" role="dialog" aria-labelledby="addModal-label" data-url="@Url.Action("DeleteCommentsView")"></div>


}

@section Scripts {
    <script id="actionButtons" type="text/x-template">
        <div>
            <a href="#editModal" data-id="${id}" title="Editar" class="tip btn btn-sm btn-light mr-2 btn-modal-action"><i class="fa fa-fw fa-edit"></i></a>
            <a href="#deleteModal" data-id="${id}" title="Eliminar" class="tip btn btn-sm btn-light btn-modal-action"><i class="fa fa-fw fa-trash"></i></a>
            
        </div>
    </script>
    <script id="contactsButtons" type="text/x-template">
        <div>
            <a href="#addContact" data-id="${id}" title="Agregar contactos" class="tip btn btn-sm btn-light btn-modal-action"> ${contacts} <i class="fa fa-fw fa-plus"></i></a>
            <a href="#viewContacts" data-id="${id}" title="Ver contactos" class="tip btn btn-sm btn-light btn-modal-action"><i class="fa fa-fw fa-id-card"></i></a>
        </div>
    </script>
    <script id="contactActionButtons" type="text/x-template">
        <div>
            <a href="#editContact" data-id="${id}" title="Editar" class="tip btn btn-sm btn-light btn-modal-action"><i class="fa fa-fw fa-edit"></i></a>
            <a href="#deleteContact" data-id="${id}" title="Eliminar" class="tip btn btn-sm btn-light btn-modal-action"><i class="fa fa-fw fa-trash"></i></a>
        </div>
    </script>
    <script id="addressesButtons" type="text/x-template">
        <div>
            <a href="#addAddress" data-id="${id}" title="Agregar dirección" class="tip btn btn-sm btn-light btn-modal-action"> ${addresses} <i class="fa fa-fw fa-plus"></i></a>
            <a href="#viewAddresses" data-id="${id}" title="Ver direcciones" class="tip btn btn-sm btn-light btn-modal-action"><i class="fa fa-fw fa-map-marked-alt"></i></a>
        </div>
    </script>
    <script id="addressActionButtons" type="text/x-template">
        <div>
            <a href="#editAddress" data-id="${id}" title="Editar" class="tip btn btn-sm btn-light btn-modal-action"><i class="fa fa-fw fa-edit"></i></a>
